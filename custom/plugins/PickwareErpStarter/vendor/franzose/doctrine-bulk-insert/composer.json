{
    "type": "library",
    "name": "franzose/doctrine-bulk-insert",
    "description": "Bulk insert functionality for the Doctrine/DBAL",
    "license": "MIT",
    "require": {
        "php": "^7.2|^8.0",
        "doctrine/dbal": "^2.5"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^8.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Franzose\\DoctrineBulkInsert\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Franzose\\DoctrineBulkInsert\\Tests\\": "tests/"
        }
    }
}
